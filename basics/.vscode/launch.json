  "version": "0.2.0",
  "configurations": [
    {
      "name": "PowerShell Launch ",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}\\r.cmd",
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via NPM",
      "runtimeExecutable": "npm",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["start", "debug"],
      "port": 9229
    }
  ]
}
